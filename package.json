{
  "name": "drive",
  "main": "expo-router/entry",
  "version": "0.0.1",
  "scripts": {
    "start": "npx expo start",
    "dev": "APP_VARIANT=development npx expo start",
    "preview": "APP_VARIANT=preview npx expo start",
    "android": "npx expo run:android --port 8082",
    "ios": "npx expo run:ios",
    "web": "expo start --web",
    "eas:android": "eas build --platform android --profile development --local",
    "eas:ios": "eas build --platform ios --profile development --local",
    "eas:ios-simulator": "eas build --platform ios --profile ios-simulator --local",
    "test": "jest --watchAll",
    "prebuild": "npx expo prebuild",
    "prebuild:clean": "npx expo prebuild --clean",
    "setProduction": "ENVFILE=.env.production",
    "build:android": "cd android && ./gradlew assembleRelease",
    "android-production": "npm run setProduction && npm run build:android",
    "android-production:full": "npm run setProduction && npx expo prebuild && npm run build:android"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@eva-design/eva": "^2.2.0",
    "@expo/vector-icons": "^14.0.2",
    "@gorhom/bottom-sheet": "^5.1.4",
    "@react-navigation/native": "^7.0.14",
    "@rnmapbox/maps": "^10.1.38",
    "@ui-kitten/components": "^5.3.1",
    "date-fns": "^4.1.0",
    "expo": "~52.0.46",
    "expo-constants": "17.0.8",
    "expo-dev-client": "~5.0.20",
    "expo-font": "~13.0.4",
    "expo-linking": "~7.0.5",
    "expo-location": "~18.0.10",
    "expo-modules-core": "2.2.3",
    "expo-router": "~4.0.20",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.9",
    "expo-web-browser": "~14.0.2",
    "lucide-react-native": "^0.511.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-paper": "^5.14.4",
    "react-native-reanimated": "~3.16.1",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "^15.12.0",
    "react-native-svg-transformer": "^1.5.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "ws": "^8.18.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/node": "^22.15.14",
    "@types/react": "~18.3.12",
    "@types/socket.io": "^3.0.2",
    "@types/ws": "^8.18.1",
    "eas-cli": "^16.7.1",
    "expo-module-scripts": "4.0.5",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.6",
    "react-test-renderer": "18.3.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.3.3"
  },
  "private": true,
  "expo": {
    "name": "drive",
    "slug": "drive",
    "ios": {
      "bundleIdentifier": "com.go.drive"
    },
    "android": {
      "package": "com.go.drive"
    },
    "scheme": "drive"
  }
}
