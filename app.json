{
  "expo": {
    "name": "drive",
    "slug": "go-cabs-and-drive",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./src/assets/images/icons/icon.png",
    "scheme": "drive",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/icons/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.go.drive"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.go.drive"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/icons/favicon.png",
      "build": {
        "babel": {
          "include": ["@ui-kitten/components"]
        }
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Lume to use your location."
        }
      ],
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fdd3b266-be2b-4347-9598-c6373b304ac4"
      }
    },
    "owner": "go-cabs"
  }
}
